#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/payflink/PayflinkHomepage/Entwicklung/gaston-webpage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/payflink/PayflinkHomepage/Entwicklung/gaston-webpage/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../module-definition@4.0.0/node_modules/module-definition/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../module-definition@4.0.0/node_modules/module-definition/bin/cli.js" "$@"
fi
